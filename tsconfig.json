{
	"compilerOptions": {
		"sourceMap": true,
		"module": "commonjs",
		"moduleResolution": "node",
		"strict": true,
		"target": "ES6",
		"outDir": "./dist",
		"lib": ["dom", "dom.iterable", "esnext"],
		"types": [],
		"baseUrl": "./node_modules",
		"jsx": "react-jsx",
		"esModuleInterop": true,
		"typeRoots": [
			"node_modules/@types"
		]
	},
	"include": [
		"./src/**/*"
	],
	"exclude": [
		"node_modules"
	]
}